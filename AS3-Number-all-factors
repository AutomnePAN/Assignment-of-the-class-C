#include <stdio.h>
#include <stdlib.h>

int main()
{
    long long int num;

    while((scanf("%lld",&num)!= EOF)){
     int count = 0;
     for(long long int i = 1; i*i<=num; i++){
        if(num%i==0){
        if(i*i == num){
                count++;
        }else{count+=2;}
        }
        }
     printf("%d\n",count);
}

    return 0;
}
